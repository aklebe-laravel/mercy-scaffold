{
    "name": "aklebe-laravel/mercy-scaffold",
    "type": "project",
    "description": "Mercy Application Scaffold",
    "keywords": [
        "framework",
        "laravel",
        "Scaffold",
        "Boilerplate"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.5",
        "ext-gd": "*",
        "chelout/laravel-relationship-events": "4.*",
        "guzzlehttp/guzzle": "7.*",
        "laravel/framework": "12.*",
        "laravel/sanctum": "4.*",
        "laravel/scout": "10.*",
        "laravel/tinker": "2.*",
        "livewire/livewire": "3.*",
        "maatwebsite/excel": "3.*",
        "mariuzzo/laravel-js-localization": "2.*",
        "nwidart/laravel-modules": "11.*",
        "opcodesio/log-viewer": "3.*",
        "predis/predis": "3.*",
        "pusher/pusher-php-server": "7.*",
        "shipu/themevel": "3.*",
        "spatie/laravel-backup": "9.*",
        "spatie/laravel-navigation": "*",
        "twbs/bootstrap-icons": "1.*",
        "wikimedia/composer-merge-plugin": "2.*"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "*",
        "barryvdh/laravel-ide-helper": "3.*",
        "brianium/paratest": "7.*",
        "fakerphp/faker": "1.*",
        "laravel/breeze": "2.*",
        "laravel/dusk": "8.*",
        "laravel/pail": "1.*",
        "laravel/pint": "1.*",
        "laravel/sail": "1.*",
        "laravel/ui": "4.*",
        "mockery/mockery": "1.*",
        "nunomaduro/collision": "^v8.1",
        "phpunit/phpunit": "12.*",
        "ryoluo/sail-ssl": "1.*",
        "spatie/laravel-ignition": "2.*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Modules\\": "Modules/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        },
        "merge-plugin": {
            "include": [
                "composer.app.json",
                "composer.local.json",
                "Modules/*/composer.json",
                "Themes/*/composer.json"
            ],
            "recurse": true,
            "replace": false,
            "ignore-duplicates": false,
            "merge-dev": true,
            "merge-extra": false,
            "merge-extra-deep": false,
            "merge-replace": true,
            "merge-scripts": false
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "wikimedia/composer-merge-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
